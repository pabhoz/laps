node{
    environment{
        CI = 'true'
    }
    stages {
        stage('Checkout') {
            steps {
                echo 'Cheking out...'
                echo 'Yes!'
            }
        }
        stage('Build') {
            steps {
                echo 'Building...'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}