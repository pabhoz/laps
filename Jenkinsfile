node{
    environment{
        CI = 'true'
    }
    stages {
        stage('Checkout') {
            steps {
                echo 'Cheking out...'
            }
        }
        stage('Build') {
            steps {
                echo 'Building...'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying...'
            }
        }
    }
}